  class InnerClass {
    int y = 5;

    public void accessOuter() {
      System.out.println("Inner Class accessing outer class variable: " + x);
    }
  }

  public void createInner() {
    InnerClass inner = new InnerClass();
    inner.accessOuter();
  }
}

